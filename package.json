{
    "name": "cosuno",
    "private": true,
    "version": "1.0.0",
    "description": "",
    "license": "MIT",
    "scripts": {
        "preinstall": "rm -rf node_modules && rm -f package-lock.json && rm -f yarn.lock",
        "graphql": "npm-run-all -p graphql:*",
        "graphql:codegen": "graphql-codegen --config .codegen.yml",
        "graphql:schema": "graphql get-schema --project cosuno --endpoint local",
        "lint": "eslint \"{src,srv}/**/*.ts\" --fix",
        "prestart": "rm -rf .cache && rm -rf .dev && rm -rf .dist",
        "start": "npm-run-all -p -r start:*",
        "start:src": "wait-on -c .waitonrc.json http-get://localhost:3000/graphql && npm run graphql && parcel src/index.html --out-dir .dev --open --port 3333",
        "start:srv": "nest start --watch",
        "upgrade": "npm-check --update --save-exact"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{ts,tsx,json,md}": [
            "prettier --write"
        ],
        "**/*.{ts,tsx}": [
            "eslint"
        ]
    },
    "dependencies": {
        "@emotion/core": "10.0.28",
        "@emotion/styled": "10.0.27",
        "@material-ui/core": "4.9.5",
        "@material-ui/icons": "4.9.1",
        "@material-ui/lab": "4.0.0-alpha.45",
        "@nestjs/common": "6.11.11",
        "@nestjs/core": "6.11.11",
        "@nestjs/graphql": "6.6.2",
        "@nestjs/platform-express": "6.11.11",
        "apollo-boost": "0.4.7",
        "apollo-cache-inmemory": "1.6.5",
        "apollo-server-express": "2.11.0",
        "change-case": "4.1.1",
        "class-validator": "0.11.0",
        "graphql": "14.6.0",
        "graphql-tools": "4.0.7",
        "lodash": "4.17.15",
        "lorem-ipsum": "2.0.3",
        "react": "16.13.0",
        "react-apollo": "3.1.3",
        "react-apollo-hooks": "0.5.0",
        "react-dom": "16.13.0",
        "react-helmet": "5.2.1",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.4",
        "type-graphql": "0.17.6",
        "uuid": "7.0.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.12.2",
        "@graphql-codegen/fragment-matcher": "1.12.2",
        "@graphql-codegen/typescript": "1.12.2",
        "@graphql-codegen/typescript-operations": "1.12.2",
        "@graphql-codegen/typescript-react-apollo": "1.12.2",
        "@nestjs/cli": "6.14.2",
        "@nestjs/schematics": "6.9.4",
        "@types/express": "4.17.3",
        "@types/graphql": "14.5.0",
        "@types/lodash": "4.14.149",
        "@types/node": "13.7.7",
        "@types/react": "16.9.23",
        "@types/react-router": "5.1.4",
        "@types/react-router-dom": "5.1.3",
        "@types/uuid": "7.0.0",
        "@typescript-eslint/eslint-plugin": "2.22.0",
        "@typescript-eslint/parser": "2.22.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-prettier": "3.1.2",
        "eslint-plugin-react": "7.18.3",
        "eslint-plugin-react-hooks": "2.5.0",
        "graphql-cli": "3.0.14",
        "husky": "4.2.3",
        "lint-staged": "10.0.8",
        "npm-check": "^5.9.2",
        "npm-run-all": "4.1.5",
        "parcel-bundler": "1.12.4",
        "prettier": "1.19.1",
        "ts-loader": "6.2.1",
        "ts-node": "8.6.2",
        "tsconfig-paths": "3.9.0",
        "typescript": "3.8.3",
        "wait-on": "4.0.1"
    }
}
